server {
    listen 80;
    listen [::]:80;
    listen 443 ssl;
    listen [::]:443 ssl;

    server_name wedding.eardley.xyz;

    root /var/www/wedding/;
    index src/index.html;

    location / {
        try_files $uri $uri/ /src/$uri /src/$uri/ =404;
    }

    access_log /var/log/nginx/wedding.access.log;
    error_log /var/log/nginx/wedding.error.log;
}